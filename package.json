  "editorconfig-cli": [
    "*.json",
    "*.js",
    "*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "^16"
  },
  "type": "module",
  "repository": {
